{
  "name": "zero-to-prod/web-framework",
  "description": "A simple web framework for PHP",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "zero-to-prod",
    "web-framework"
  ],
  "readme": "./README.md",
  "time": "2025-11-29",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com",
      "homepage": "https://davidsmith.blog/",
      "role": "maintainer"
    }
  ],
  "homepage": "https://github.com/zero-to-prod/web-framework",
  "support": {
    "email": "dave0016@gmail.com",
    "issues": "https://github.com/zero-to-prod/web-framework/issues",
    "source": "https://github.com/zero-to-prod/web-framework",
    "docs": "https://zero-to-prod.github.io/web-framework/",
    "security": "https://github.com/zero-to-prod/web-framework/blob/main/SECURITY.md"
