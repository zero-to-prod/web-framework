    "nyholm/psr7": "^1.0",
    "nyholm/psr7-server": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "<=11.0.0"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\WebFramework\\": "src/"
    }
  },
  "autoload-dev": {
